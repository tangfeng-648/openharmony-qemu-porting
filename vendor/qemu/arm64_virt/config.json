{
  "product_name": "arm64_virt",
  "device_company": "qemu",
  "device_build_path": "device/board/qemu/arm64_virt",
  "target_cpu": "arm",
  "type": "standard",
  "version": "3.0",
  "board": "arm64_virt",
  "api_version": 9,
  "enable_ramdisk": true,
  "inherit": [ "productdefine/common/inherit/rich.json", "productdefine/common/inherit/chipset_common.json" ],
  "subsystems": [
