        {
          "component": "bluetooth_service",
          "features": []
        }
      ]
    },
    {
      "subsystem": "hdf",
      "components": [
        {
          "component": "drivers_interface_ril",
          "features": []
        },
        {
          "component": "drivers_peripheral_ril",
          "features":[]
        }
      ]
    },
    {
      "subsystem": "qemu_products",
      "components": [
        {
          "component": "qemu_products",
          "features": []
        }
      ]
    },
    {
      "subsystem": "arkui",
      "components": [
        {
          "component": "ace_engine",
